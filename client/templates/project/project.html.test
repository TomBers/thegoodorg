<template name="projectCard">

      <div class="project-card left">
        <div style="width:430px;height:50px;">

        <row padding-top='5px' padding-bottom='5px'>
            {{#with linkCompanyOwner}}
                <img class="profile-pic" src="{{user_photo}}" style='margin-bottom:2px' >

              <div class="col-sm-6">

                  {{name}}
                  <br>{{user_title}}
              </div>
            {{/with}}

                <div>
                  {{#each getInterestPic}}
                  <img class="project-icon" src="{{this}}" align='middle'/>
                  {{/each}}
              </div>

        </row>
      </div>

        <div div style="width:430px;height:200px;">

        <center>
          <img class='photobox' src="{{img}}" height ='100%' width='auto'/>
        </center>
        </div>


          <div class="fadeout">
            {{desc}}
          </div>


        <span class='goals' style="height:20px">

        {{#if impact_h}}
          <img class="icon" src="/images/icons/icon-H-impact.png" /><span class="blue" >{{impact_h}}</span>
        {{/if}}

        {{#if impact_e}}
          <img class="icon" src="/images/icons/icon_E-impact.png"  /><span class="green">{{impact_e}}</span>
        {{/if}}

        {{#if impact_r}}
          <img class="icon" src="/images/icons/icon-R-impact.png" /><span class="yellow">{{impact_r}}</span>
        {{/if}}

        </span>

        <div class="clear">      </div>

        <div  padding-bottom='3px'>

        <div class="location">
          <div class="container" style="margin:0; padding:0 5px 0 5px">
             <span style='float:left'>{{linkCompany.name}}</span>
             <span style='float:right'><strong>{{city}}, {{country}}</strong></span>
          </div>

        </div>
          <a type="button" class="btn btn-info btn-xs btn-block" id='moreInfo' href="/project/{{_id}}">Learn More</a>
        </div>


      </div>



  </template>




