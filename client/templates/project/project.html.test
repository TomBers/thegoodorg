<template name="projectPanelLeft">
<div class="col-md-3" style='border:1px solid blue;'>

  <!-- <div class="left-side-panel" width='200px'> -->
    <div style='text-align:left;'>
    <!-- <div class="container" text-align='left'> -->

    <br  /> <strong style='color:blue;'> End Date: {{formatDate endDate}}</strong>
    <br  /> Start Date: <strong>{{formatDate startDate}}</strong>
    <br  />Duration: <strong> {{duration}} </strong>
    <br>
      <br  /> Industry: <strong>{{linkCompany.industry}}</strong>
      <div class="clear"> </div>
      <br>
    </div>

    {{#each interactions}}

    <div class="project-info">
      <center>
        <img src="{{getInterestPic this}}" align='middle'/>
      </center>
      <h7>{{this}}</h7>

    </div>

    {{/each}}

  </div>
<!-- </div>  -->
</template>

<template name="projectPanelCentre">
